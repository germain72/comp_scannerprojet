<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/jsp/jstl/core http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletData</servlet-name>
        <servlet-class>ch.conceptforge.mavenprojecthibernatetuto.ServletData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MaServletData</servlet-name>
        <servlet-class>ch.conceptforge.mavenprojecthibernatetuto.MaServletData</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletData</servlet-name>
        <url-pattern>/ServletData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MaServletData</servlet-name>
        <url-pattern>/MaServletData</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EmployeeController</servlet-name>
        <servlet-class>ch.conceptforge.mavenprojecthibernatetuto.Controller.EmployeeController</servlet-class>
    </servlet>   
    <servlet-mapping>
        <servlet-name>EmployeeController</servlet-name>
        <url-pattern>/Employee</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
